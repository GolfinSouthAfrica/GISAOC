<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" focusTraversable="true" prefHeight="700.0" prefWidth="1100.0" spacing="20.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.LoadingPaneController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resources/GISALogo.png" />
         </image></ImageView>
      <Label fx:id="appHeadingLbl" styleClass="heading-text" stylesheets="@../resources/GISAStyle.css" text="Golf in South Africa" />
      <Label styleClass="home-text" stylesheets="@../resources/GISAStyle.css" text="AN UNFORGETTABLE STORY" />
      <ProgressIndicator prefHeight="137.0" prefWidth="613.0" style="-fx-progress-color: #1a6331;" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
