<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.SuppliersPaneController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="78.0" prefWidth="1100.0">
         <children>
            <Label styleClass="mainHeadingText" stylesheets="@../resources/GISAStyle.css" text="Golf in South Africa" />
            <Label styleClass="secondaryHeadingText" stylesheets="@../resources/GISAStyle.css" text="Suppliers" />
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="120.0" prefWidth="1100.0" spacing="20.0">
         <children>
            <TextField fx:id="searchTxf" styleClass="products-searchfield" stylesheets="@../resources/GISAStyle.css" />
            <Button mnemonicParsing="false" onAction="#searchButtonClick" styleClass="dialog-button" stylesheets="@../resources/GISAStyle.css" text="Search" />
            <Label styleClass="tertiaryHeadingText" stylesheets="@../resources/GISAStyle.css" text="Sort by:" />
            <ComboBox fx:id="sortBy" prefWidth="150.0" styleClass="sortby-combo-box" stylesheets="@../resources/GISAStyle.css" />
         </children>
         <VBox.margin>
            <Insets top="15.0" />
         </VBox.margin>
      </HBox>
      <ScrollPane fx:id="suppliersScrollPane" prefHeight="400.0" prefWidth="1100.0">
         <content>
            <VBox fx:id="suppliersList" prefHeight="340.0" prefWidth="1020.0" />
         </content>
         <VBox.margin>
            <Insets bottom="10.0" left="30.0" right="30.0" top="10.0" />
         </VBox.margin>
      </ScrollPane>
      <HBox alignment="CENTER" prefHeight="80.0" prefWidth="794.0" spacing="820.0">
         <children>
            <StackPane onMouseClicked="#addButtonClick" prefHeight="80.0" prefWidth="90.0" styleClass="home-button" stylesheets="@../resources/GISAStyle.css">
               <children>
                  <ImageView fitHeight="80.0" fitWidth="90.0" onMouseClicked="#addButtonClick" pickOnBounds="true" preserveRatio="true" />
                  <Label onMouseClicked="#addButtonClick" styleClass="home-button-text" stylesheets="@../resources/GISAStyle.css" text="Add" />
               </children>
            </StackPane>
            <StackPane onMouseClicked="#backButtonClick" prefHeight="80.0" prefWidth="90.0" styleClass="home-button" stylesheets="@../resources/GISAStyle.css">
               <children>
                  <ImageView fitHeight="80.0" fitWidth="90.0" onMouseClicked="#backButtonClick" pickOnBounds="true" preserveRatio="true" />
                  <Label onMouseClicked="#backButtonClick" styleClass="home-button-text" stylesheets="@../resources/GISAStyle.css" text="Back" />
               </children>
            </StackPane>
         </children>
         <VBox.margin>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets top="10.0" />
   </padding>
</VBox>
